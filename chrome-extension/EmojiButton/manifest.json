{
  "name": "Emoji Button",
  "description": "append emoji button after every edittext",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons":{
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "app.css"
      ],
      "js": [
        "jquery-1.9.0.min.js",
        "app.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": ["icon.png"],
  "manifest_version": 2
}