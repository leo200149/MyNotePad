{
  "name": "Regex",
  "description": "You can select by Regex every where",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "jquery-1.9.0.min.js",
      "typing.js"
    ]
  },
  "icons":{
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "jquery.fancybox.min.css",
        "popup.css"
      ],
      "js": [
        "jquery-1.9.0.min.js",
        "jquery.fancybox.min.js",
        "popup.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}